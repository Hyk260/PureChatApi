{
  "name": "pure-backend",
  "version": "1.0.1",
  "description": "PureChat API",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --config nodemon.json index.js",
    "build": "node src/app.js",
    "serve": "node index.js",
    "start": "nodemon --config nodemon.json src/app.js",
    "build:pkgwin": "pkg . -t node16-win-x64 -o dist/app",
    "build:pkgmac": "pkg . -t node14-macos-x64 -o dist/app"
  },
  "dependencies": {
    "@vercel/node": "^2.8.4",
    "ai": "^3.0.24",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.12.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.0.3",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.3",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.38.1",
    "pkg": "^5.8.0",
    "tls-sig-api-v2": "^1.0.2",
    "uuid": "^8.3.2",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "eslint": "8.7.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.20",
    "prettier": "^3.2.5"
  },
  "pkg": {
    "scripts": "module/*.js",
    "targets": [
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "assets": [
      "node_modules/axios",
      "/public",
      ".env",
      ".env.local"
    ],
    "outputPath": "dist"
  }
}
